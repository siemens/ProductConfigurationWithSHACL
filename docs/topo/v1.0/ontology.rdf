    <rdfs:Datatype rdf:about="https://w3id.org/rail/topo#Navigability">
        <rdfs:label>Navigability</rdfs:label>
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
