        <rdfs:domain rdf:resource="https://w3id.org/rail/topo#PositionedRelation"/>
        <rdfs:range rdf:resource="https://w3id.org/rail/topo#Usage"/>
        <rdfs:label>positionOnA</rdfs:label>
        <skos:definition>if &quot;0&quot; then the &quot;Relation&quot; is using the start of NetElement A
if &quot;1&quot; then the &quot;Relation&quot; is using the end of NetElement A</skos:definition>
    </owl:DatatypeProperty>
    


