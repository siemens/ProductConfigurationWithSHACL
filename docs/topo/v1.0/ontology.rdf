        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/rail/topo#LinearAnchorPoint"/>
                    <rdf:Description rdf:about="https://w3id.org/rail/topo#LinearCoordinate"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:label>measure</rdfs:label>
        <skos:definition xml:lang="en">For LinearCoordinate: location at the &quot;line of reference&quot; (possibly adjusted to local anomalies using &quot;LinearAnchorPosition&quot;)
For LinearAnchorPoint: Measure of the Anchor Point within the given“LinearPositioningSystem”</skos:definition>
    </owl:DatatypeProperty>
    


