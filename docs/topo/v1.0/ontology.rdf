        <rdfs:range rdf:resource="https://w3id.org/rail/topo#NetElement"/>
        <rdfs:label>element</rdfs:label>
        <ns:term_status>unstable</ns:term_status>
        <skos:definition>superproperty of [elementA](#elementA) and [elementB](#elementB) to infer inverse [relation](#relation)</skos:definition>
    </owl:ObjectProperty>
    


